#!/bin/sh

pcvs_src_current=$pcvs_src/$1
pcvs_build_current=$pcvs_testbuild/$1


for f in `ls -1 $pcvs_src_current`
do
	name=`echo "$f" | sed -e "s,\..*$,,"`
	printf "$name:
    group: 'GRPMPI'
    build:
        files: '@SRCPATH@/$f'
    run:
        iterate:
            n_mpi:
                values: [{op: 'seq', 'from': 2}]
"
done
exit 0
